<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kwizera.javaamalilabs03bankmgtsystem.controllers.MainMenuController">
    <Label fx:id="helloLabel" alignment="CENTER" layoutX="34.0" layoutY="27.0" prefHeight="35.0" prefWidth="534.0"
           text="Hello">
        <font>
            <Font name="System Bold" size="24.0"/>
        </font>
    </Label>
    <Button fx:id="withDrawBtn" layoutX="54.0" layoutY="175.0" mnemonicParsing="false" prefHeight="97.0"
            prefWidth="131.0" text="WITHDRAW" onAction="#onWithdrawClicked"/>
    <Button fx:id="depositBtn" layoutX="235.0" layoutY="175.0" mnemonicParsing="false" prefHeight="97.0"
            prefWidth="131.0" text="DEPOSIT" onAction="#onDepositClicked"/>
    <Button fx:id="historyBtn" layoutX="410.0" layoutY="175.0" mnemonicParsing="false" prefHeight="97.0"
            prefWidth="131.0" text="HISTORY" onAction="#onHistoryClicked"/>
    <Button fx:id="logoutBtn" layoutX="274.0" layoutY="332.0" mnemonicParsing="false" onAction="#onLogoutClicked"
            text="Log out"/>
    <Label fx:id="accNumberLabel" alignment="CENTER" layoutX="36.0" layoutY="75.0" prefHeight="35.0" prefWidth="534.0">
        <font>
            <Font size="18.0"/>
        </font>
    </Label>
    <Label fx:id="accBalanceLabel" alignment="CENTER" layoutX="33.0" layoutY="110.0" prefHeight="35.0"
           prefWidth="534.0">
        <font>
            <Font size="18.0"/>
        </font>
    </Label>
</AnchorPane>
